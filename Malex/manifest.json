{
    "manifest_version": 2,

    "name": "Malex",
    "description": "Gimme da stuffz",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "extract.html"
    },
    "content_scripts": [{
        "matches":    ["*://*/*"],
        "js": ["externalJS/jquery.js","formdata.js"]
    }],
    "permissions": [
        "activeTab",
        "tabs",
        "history",
        "identity",
        "identity.email",
        "storage"
    ]
}